using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace wsbLab2
{
    class PointClass
    {

        public int X { get; set; }

        public int Y { get; set; }

        public PointClass(int X, int Y)
        {

            this.X = X;
            this.Y = Y;
        }

    }
}
